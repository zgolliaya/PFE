pipeline {
        agent any

        stages {
            stage('Build') {
                steps {
                    echo 'Build starting...'
                    git url: "https://github.com/zgolliaya/PFE.git"
                    sh(script:"pip3 install robotframework")
                    sh(script:"pip3 install robotframework-requests")
                }
            }
            stage('Test') {
                steps {
                    echo 'Test starting...'
                    sh(script:"robot -d ./test1/APItest.robot")
                }
            }
        }
    }